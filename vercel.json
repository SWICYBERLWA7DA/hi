{
  "functions": {
    "api/hy.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/hi/hy.py",
      "destination": "/api/hy.py"
    },
    {
      "source": "/hi/hy",
      "destination": "/api/hy.py"
    },
    {
      "source": "/hy",
      "destination": "/api/hy.py"
    },
    {
      "source": "/(.*)",
      "destination": "/api/hy.py"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Cache-Control", "value": "no-store, no-cache, must-revalidate, max-age=0" }
      ]
    }
  ]
}
